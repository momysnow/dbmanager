# DBManager environment variables
DBMANAGER_CREATE_ADMIN=true
DBMANAGER_ADMIN_USER=admin
DBMANAGER_ADMIN_PASSWORD=change-me
# JWT secret for API tokens (recommended to set)
DBMANAGER_JWT_SECRET=change-me-jwt-secret
# Master key for encrypting sensitive config values (Fernet key)
# Generate with: python -c "from cryptography.fernet import Fernet; print(Fernet.generate_key().decode())"
DBMANAGER_MASTER_KEY=
